<templateSet group="PHP">
  <template name="con" value="public function __construct($PARAMETERS$){&#10;    $END$&#10;}" description="public function" toReformat="true" toShortenFQNames="true">
    <variable name="PARAMETERS" expression="phpVar" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="@test" value="/** @test */&#10;public function $NAME$(): void&#10;{&#10;    $END$&#10;}" description="Unit Test" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP Class Member" value="true" />
    </context>
  </template>
  <template name="uuid" value="$VALUE$" description="Generate random UUID" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="groovyScript(&quot;java.util.UUID.randomUUID() as String&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>