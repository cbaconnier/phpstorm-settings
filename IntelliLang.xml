<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JavaScript" injector-id="js">
      <display-name>x-init</display-name>
      <prefix>x-init=&quot;</prefix>
      <suffix>&quot;</suffix>
    </injection>
    <injection language="JSON" injector-id="xml">
      <display-name>script</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("script")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>